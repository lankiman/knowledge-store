@{
    Layout = null;
}
@model e_learning.DataTransfersObjects.UserDto

<h1>Welcome to Admin Dashboard</h1>

@if (Model != null)
{
    <p class="">Admin User ID: @Model.Id</p>
    <p>Admin User Name: @Model.UserName</p>
    <button onclick="location.href='@Url.Action("Logout", "Account")'">Logout</button>

}
else
{
    <p>No admin user data available.</p>
}

